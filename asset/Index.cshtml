<html class="no-js" data-ng-app="storeApp">
<!--<![endif]-->
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta charset="utf-8">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0">                 

  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">                             
  <meta name="format-detection" content="telephone=no">

  <meta name="author" content="Grocery Shopping Network">
  <meta name="description" content="Shop for groceries with our meal planning solutions, save hundreds of dollars with printable coupons and find all recipes and meal ideas.">
  <meta name="keywords" content="Weekly Specials, Recipes, Coupons, Meal Planning, Cooking Videos, Low Prices, Shop Online">     

  <title>@this.ViewBag.Title</title>
  <!--[if lt IE 8]><meta http-equiv="refresh" content="5;URL=/unsupported-browser.htm" /><![endif]-->

  <link href="@this.ViewBag.CdnUrl/script/lib/twitter-bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" type="text/css"> 
  <link href="@this.ViewBag.CdnUrl/asset/common/styles/global.css" rel="stylesheet" type="text/css">    
  <link href="//cdnjs.cloudflare.com/ajax/libs/qtip2/2.2.0/jquery.qtip.min.css" rel="stylesheet" type="text/css"><!-- for circular imagemap -->    
  <link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">                      
  <link href="//cdnjs.cloudflare.com/ajax/libs/blueimp-gallery/2.15.2/css/blueimp-gallery.min.css" rel="stylesheet" type="text/css">   
  <link href="@this.ViewBag.CdnUrl/script/lib/bootstrap-image-gallery.min.css" rel="stylesheet" type="text/css">        
  <link href="@this.ViewBag.AppCss" rel="stylesheet" type="text/css">  
  <script>var globalConfig = { data: {}, apiUrl: '@Gsn.Digital.Web.MvcApplication.ProxyMasterUrl' };</script>
  <!--[if lt IE 10]>
    <script src="@this.ViewBag.CdnUrl/script/lib/proxy/xdomain.min.js" data-slave="@this.ViewBag.CdnUrl/script/lib/proxy/proxy.html"></script>  
  <![endif]--><!--endhead-->
</head>
<body data-ctrl-body="" data-ng-class="{ 'isLoggedIn': isLoggedIn, 'app-ready': (appState == 'ready' )}">
  <div data-bs-display-mode="bsDisplayMode"><!-- do not hide this div, it is use to detect display size --></div> 
  <div class="app-loading-container" data-ng-hide="appState == 'ready'">
    <img id="loading-image" src="@this.ViewBag.CdnUrl/script/images/loading.gif" alt="Loading..." />
  </div>  
                                  
  <!-- sticky message -->                      
  <div class="container" data-gsn-ft-config="message-top">
    <div class="row" data-gsn-sticky="" data-fixed-width="true" data-ng-if="item.Description" data-ng-bind-html="item.Description | trustedHtml"></div>
  </div>                     

  <!-- the main container -->
  <div class="layout-container" data-ng-include="currentLayout"></div>    
  <a class="back-to-top" data-ng-scroll-top="scrollTop" data-offset="50"><i class="fa fa-chevron-up"></i><br />Top</a>                           
  <div class="hidden" data-gsn-path-pixel="https://secure.adnxs.com/getuid?https://dataapi.gsn2.com/api/v1/partner/linkadnx/[[ProfileId]]/[[ChainId]]?adnxs_uid=$UID&nocache=[[CACHEBUSTER]]"></div>
                                                                                                    
  <!-- Unfortunately, this cannot be injected as Coupons, Inc. required this to be inside the html before document load.  Plugin permission issue. -->
  <div class="hidden"><iframe id="ci_ic1" name="ci_ic1" height="0" width="0" style="position: absolute; top: -9999em; left: -9999em; width: 0px; height: 0px; border: 0; z-index: 99;"></iframe></div>

  <!-- above the fold scripts -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>        
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery-migrate/1.2.1/jquery-migrate.min.js"></script><!-- support for plugin that were written for jquery below 1.9 -->  
  <script src="//cdnjs.cloudflare.com/ajax/libs/handlebars.js/1.3.0/handlebars.min.js"></script><!-- for circular imagemap --> 
  <script src="//cdnjs.cloudflare.com/ajax/libs/blueimp-gallery/2.15.2/js/jquery.blueimp-gallery.min.js"></script><!-- for image gallery popup -->    

  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular.min.js"></script>                                             
  <script src="@this.ViewBag.CdnUrl/script/gcprinter/gcprinter.min.js"></script>                                         
  <script data-gsnid="@this.ViewBag.ChainId" src="@this.ViewBag.CdnUrl/script/gsndfp/gsndfp.min.js"></script>  
  <script src="@this.ViewBag.CdnUrl/script/gsncore/@Gsn.Digital.Web.MvcApplication.AppVersion.Replace("_", ".")/gsncore.min.js"></script>       

  <!-- begin content -->             
  <script src="@this.ViewBag.DataUrl"></script>      
  <script src="@this.ViewBag.AppUrl"></script>                                           
                                                                       
  <!-- polyfills: html5, es5, and json -->                  
  <!--[if lt IE 10]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7/html5shiv.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/2.2.0/es5-shim.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/2.2.0/es5-sham.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/json2/20130526/json2.min.js"></script>
  <![endif]-->                                                                 
                                                   
  <!-- angular depencencies -->                                             
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular-sanitize.min.js"></script><!-- security -->   
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular-route.min.js"></script><!-- routing and history -->  
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular-animate.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular-touch.min.js"></script> 

  <!-- below the fold: js -->                                                                                 
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script><!-- bootstrap -->    
  <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.10.0/ui-bootstrap-tpls.min.js"></script><!-- bootstrap to angular bridge -->               
  <script src="//cdnjs.cloudflare.com/ajax/libs/qtip2/2.2.0/jquery.qtip.min.js"></script><!-- for circular image map -->
  <script src="//cdnjs.cloudflare.com/ajax/libs/spin.js/1.3.2/spin.min.js"></script><!-- loading spinner -->  
  <!-- google map -->     
  <script src="//maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&libraries=geometry"></script>

